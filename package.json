{
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && cross-env NODE_ENV=production webpack",
    "build:dev": "npm run clean && webpack --env environment=development",
    "build:watch": "npm run clean && webpack --env environment=development --watch",
    "build:digitalocean": "npm install --production=false && npm run build && npm prune --production && npm run start",
    "start": "node ./dist/server/index.js",
    "start:dev": "concurrently \"webpack serve --hot\" \"npm run start\"",
    "test:react": "jest --detectOpenHandles --config jest.config.react.js",
    "test:node": "jest --detectOpenHandles --config jest.config.node.js",
    "test": "npm run test:react && npm run test:node"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.13",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mongoose": "^5.11.97",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-refresh": "^0.14.2",
    "react-refresh-typescript": "^2.0.9",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "express": "^4.19.2",
    "mongoose": "^8.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0"
  }
}
